{
  "name": "bkg.forge-backend",
  "version": "1.0.0",
  "description": "Backend for bkg.forge cyberpunk local AI development platform",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "lint": "eslint \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@prisma/client": "5.15.0",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "helmet": "7.1.0",
    "node-fetch": "3.3.2",
    "pg": "8.11.3",
    "pino": "8.17.0",
    "pino-pretty": "10.3.1",
    "prisma": "5.15.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.14.9",
    "@types/uuid": "9.0.7",
    "eslint": "8.57.0",
    "eslint-config-standard-with-typescript": "37.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.2.0",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "tsx": "4.14.0",
    "typescript": "5.4.5"
  }
}
